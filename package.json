{
  "name": "ngx-d3-tooltip",
  "description": "Add tooltips to your d3 visualizations using Angular Components.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "compodoc src/app/modules/d3-tooltip -p ./tsconfig.json -y docs-styles/ -d docs --hideGenerator",
    "docs-coverage": "compodoc src/app/modules/d3-tooltip --coverageTest -p ./tsconfig.json -d docs -s -w --theme stripe"
  },
  "private": false,
  "dependencies": {
    "@angular/core": "^5.0.0",
    "@angular/common": "^5.0.0",
    "d3": "^4.11.0"
  },
  "devDependencies": {
    "@angular/animations": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@compodoc/compodoc": "^1.0.4",
    "core-js": "^2.4.1",
    "request-progress": "^3.0.0",
    "rxjs": "^5.5.2",
    "zone.js": "^0.8.14",
    "@angular/cli": "^1.5.2",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/language-service": "^5.0.0",
    "@types/d3": "^4.11.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
