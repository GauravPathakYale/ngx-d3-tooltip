{
  "name": "ngx-d3-tooltip",
  "description": "Add tooltips to your d3 visualizations using Angular Components.",
  "version": "0.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/andyperlitch/ngx-d3-tooltip"
  },
  "author": {
    "name": "Andy Perlitch",
    "url": "http://andyperlitch.net"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /ngx-d3-tooltip/demo/ --output-path docs/demo",
    "test": "ng test -cc -sr",
    "test-watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "packagr": "ng-packagr -p ng-package.json",
    "docs": "compodoc src/app/modules/d3-tooltip -p ./tsconfig.json -y docs-styles/ -d docs --hideGenerator",
    "docs-coverage": "compodoc src/app/modules/d3-tooltip --coverageTest -p ./tsconfig.json -d docs -s -w --theme stripe"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "^6.1.0-beta.3",
    "@angular/core": "^6.1.0-beta.3",
    "d3": "^4.11.0"
  },
  "devDependencies": {
    "@angular/animations": "^6.1.0-beta.3",
    "@angular/cli": "^6.0.8",
    "@angular/compiler": "^6.1.0-beta.3",
    "@angular/compiler-cli": "^6.1.0-beta.3",
    "@angular/forms": "^6.1.0-beta.3",
    "@angular/http": "^6.1.0-beta.3",
    "@angular/language-service": "^6.1.0-beta.3",
    "@angular/platform-browser": "^6.1.0-beta.3",
    "@angular/platform-browser-dynamic": "^6.1.0-beta.3",
    "@angular/router": "^6.1.0-beta.3",
    "@compodoc/compodoc": "^1.0.4",
    "@types/d3": "^4.11.1",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "core-js": "^2.4.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "protractor": "~5.1.2",
    "request-progress": "^3.0.0",
    "rxjs": "^6.2.1",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.8.4",
    "zone.js": "^0.8.26",
    "@angular-devkit/build-angular": "~0.6.8"
  }
}
